{
  "extends": [
    "development"
  ],
  "hints": {
    "disown-opener": "off",
    "axe/text-alternatives": "off",
    "compat-api/html": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-justify",
          "text-justify: inter-word",
          "text-align-last",
          "backdrop-filter",
          "min-height: auto"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "no-inline-styles": "off"
  }
}